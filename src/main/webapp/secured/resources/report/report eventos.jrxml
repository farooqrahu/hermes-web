	<queryString>
		<![CDATA[SELECT
     evento.`descripcion` AS evento_descripcion,
     evento.`fecha_inicio` AS evento_fecha_inicio,
     evento.`fecha_fin` AS evento_fecha_fin,
     evento.`observaciones` AS evento_observaciones,
CONCAT(asignatura.`nombre`, ' ',  asignatura.`curso`)AS asignatura_curso,
     tipo_evento.`descripcion` AS tipo_evento_descripcion,
CONCAT(person.`nombre`, ' ', person.`apellido1`) AS person_1
FROM
     `asignatura` asignatura INNER JOIN `evento` evento ON asignatura.`id_asignatura` = evento.`id_asignatura`
     INNER JOIN `tipo_evento` tipo_evento ON evento.`id_tipo_evento` = tipo_evento.`id_tipo_evento`
     INNER JOIN `person` person ON evento.`id_organizador` = person.`id_person`]]>
	</queryString>
