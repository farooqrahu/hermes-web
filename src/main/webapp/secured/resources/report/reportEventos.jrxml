	<queryString>
		<![CDATA[SELECT
     evento.`descripcion` AS descripcion,
     evento.`fecha_inicio` AS fechaInicio,
     evento.`fecha_fin` AS fechaFin,
     evento.`observaciones` AS observaciones,
CONCAT(asignatura.`nombre`, ' [',  asignatura.`curso`, ']') AS asignatura,
     tipo_evento.`descripcion` AS tipoEvento,
CONCAT(persona.`nombre`, ' ', persona.`apellido1`) AS organizador
FROM
     `asignatura` asignatura INNER JOIN `evento` evento ON asignatura.`id_asignatura` = evento.`id_asignatura`
     INNER JOIN `tipo_evento` tipo_evento ON evento.`id_tipo_evento` = tipo_evento.`id_tipo_evento`
     INNER JOIN `persona` persona ON evento.`id_organizador` = persona.`id_persona`]]>
	</queryString>
